{
  "name": "doublyai",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"node --env-file=.env --watch server/index.js\"",
    "build": "vite build",
    "start": "NODE_ENV=production node server/index.js",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:browser": "vitest run --config vitest.browser.config.js",
    "test:server": "vitest run --config vitest.server.config.js",
    "test:ci": "npm run test && npm run test:server && npm run test:browser"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@aws-sdk/client-s3": "^3.985.0",
    "express": "^4.21.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "concurrently": "^9.1.0",
    "jsdom": "^28.0.0",
    "playwright": "^1.58.2",
    "vite": "^6.0.0",
    "vitest": "^4.0.18"
  }
}
